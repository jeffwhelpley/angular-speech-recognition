{
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "semis": true,
    "bracketSpacing": true,
    "allowParens": "avoid",
    "trailingComma": "es5",
    "bracketSameLine": false,
    "printWidth": 140,
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "angular"
            }
        }
    ]
}
